{
  "name": "Proxy Anywhere",
  "version": "1.1.0",
  "minimum_chrome_version": "13",
  "description": "Proxy Anywhere allows you to instantaneously change to your custom proxy from a click of a button.",
  "permissions": [ "proxy" ],
  "background_page": "background.html",
  "options_page": "options.html",
  "browser_action": {
   "default_icon": "img/offline.png"
  },
  "icons": {
    "16": "img/online.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  }
}
